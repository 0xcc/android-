    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="100px"
    android:paddingRight="20px"
    android:paddingTop="120px"
    android:paddingBottom="20px">

    <mike.mylauncher.CCTableLayout.LauncherLayout
        xmlns:cclayout="http://schemas.android.com/apk/res-auto"
        android:background="@drawable/bg2"

        android:id="@+id/mycclayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:paddingLeft="40px"
        android:paddingRight="40px"
        android:paddingTop="140px"
        android:paddingBottom="40px"

        cclayout:cellPadding="10"

        cclayout:rows="5"
        cclayout:columns="4"
        cclayout:dragable="true">
        >


    </mike.mylauncher.CCTableLayout.LauncherLayout>

</LinearLayout>